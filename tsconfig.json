{
  "compilerOptions": {
    "module": "ES6",
    "target": "ES6",
    "outDir": "./static/js",
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "alwaysStrict": true,
    "strictNullChecks": true
  },
  "include": [
    "src/js/**/*",
    "global.d.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}